#!/bin/bash

alias spec="clear; neofetch --ascii_colors 3 3 --colors 3 3 7 7 7 7"
alias specs="clear; neofetch | lolcat -S 34"
alias luna="curl wttr.in/moon"
alias tks="tmux kill-server"
alias cb="xclip -selection clipboard"
alias v="xclip -o"
alias open="xdg-open"
alias pipes="pipes.sh -p 10 -r 40000"
alias path="echo $PATH | sed 's/:/\n/g' | sort | uniq"
alias games="steam &> /dev/null &"
alias tunes="spotify &> /dev/null &"
alias yeet="rm -rf"
alias stat="ytop -p"
